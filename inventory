
# Please configure beaker_server to use an IP.
# As dci-rhel-agent use a container, please ensure ssh key is located
# at `/etc/dci-rhel-agent/secrets/id_rsa` if needed.

beaker_server ansible_host=x.x.x.x ansible_ssh_user=root ansible_ssh_private_key_file=/etc/dci-rhel-agent/secrets/id_rsa

